{
	"name": "code-md-memos",
	"displayName": "code-md-memos",
	"publisher": "mstszk",
	"description": "Add some extensions to VS Code built-in markdown preview",
	"private": true,
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"markdown.markdownItPlugins": true,
		"markdown.previewStyles": [
			"./styles/mdstyle.css"
		],
		"commands": [
			{
				"command": "extension.code-md-memos.print",
				"title": "Markdown Print (code-md-memos)"
			}
		],
		"configuration": {
			"type": "object",
			"title": "code-md-memos",
			"properties": {
				"code-md-memos.distDir": {
					"type": "string",
					"default": ".",
					"description": "Specifies the dist directory."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/markdown-it": "^10.0.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@neilsustc/markdown-it-katex": "^0.5.0",
		"bibtex": "^0.9.0",
		"markdown-it": "^11.0.0",
		"markdown-it-attrs": "^3.0.2",
		"markdown-it-bracketed-spans": "^1.0.1",
		"markdown-it-container": "^2.0.0",
		"markdown-it-emoji": "^1.4.0",
		"markdown-it-imsize": "^2.0.1",
		"markdown-it-mark": "^3.0.0",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-underline": "^1.0.1",
		"mustache": "^4.0.1",
		"puppeteer": "^3.3.0"
	}
}
